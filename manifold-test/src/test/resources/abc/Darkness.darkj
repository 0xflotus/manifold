package abc;

public class Darkness
{
  private String _name;

  public Darkness( String name )
  {
    _name = name;
  }

  public String getName()
  {
    return _name;
  }

  public Inner makeStuff( String stuff )
  {
    return new Inner( stuff );
  }

  public class Inner
  {
    private String _stuff;

    public Inner( String stuff )
    {
      _stuff = stuff;
    }

    public String getStuff()
    {
      return _stuff;
    }
  }
}